<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Expolerer"
        xmlns:Wpf="clr-namespace:Microsoft.Web.WebView2.Wpf;assembly=Microsoft.Web.WebView2.Wpf" x:Name="window" x:Class="Expolerer.MainWindow"
        mc:Ignorable="d"
        Title="加载中..." Height="450" Width="800">
    <Grid>
        <TextBox x:Name="LinkTypeBox" HorizontalAlignment="Center" Text="" VerticalAlignment="Top" Panel.ZIndex="1"/>
        <Wpf:WebView2 x:Name="View" Panel.ZIndex="0" Height="300"/>
        <Button x:Name="EnterButton" Content="访问" HorizontalAlignment="Right" Margin="757,0,0,0" VerticalAlignment="Top" Click="EnterButton_Click"/>
        <Button x:Name="GoBackButton" Content="←" HorizontalAlignment="Left" VerticalAlignment="Top" Click="GoBackButton_Click" IsEnabled="False"/>
        <Button x:Name="GoForwardButton" Content="→" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="16,0,0,0" IsEnabled="False" Click="GoForwardButton_Click"/>
    </Grid>
</Window>
